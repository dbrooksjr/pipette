{
    "multipart": [
        {
            "when": {
                "powered": false
            },
            "apply": {
                "model": "pipette:block/pipe_ender"
            }
        },
        {
            "when": {
                "powered": true
            },
            "apply": {
                "model": "pipette:block/pipe_ender"
            }
        },
        {
            "when": {
                "north": "true"
            },
            "apply": {
                "model": "pipette:block/pipe_extension"
            }
        },
        {
            "when": {
                "east": "true"
            },
            "apply": {
                "model": "pipette:block/pipe_extension",
                "y": 90
            }
        },
        {
            "when": {
                "south": "true"
            },
            "apply": {
                "model": "pipette:block/pipe_extension",
                "y": 180
            }
        },
        {
            "when": {
                "west": "true"
            },
            "apply": {
                "model": "pipette:block/pipe_extension",
                "y": 270
            }
        },
        {
            "when": {
                "up": "true"
            },
            "apply": {
                "model": "pipette:block/pipe_extension",
                "x": -90
            }
        },
        {
            "when": {
                "down": "true"
            },
            "apply": {
                "model": "pipette:block/pipe_extension",
                "x": 90
            }
        }
    ]
}